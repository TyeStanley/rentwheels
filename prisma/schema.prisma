generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String   @unique
  email     String   @unique
  password  String
  picture   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  car       Car[]
}

model Car {
  id        String   @id @default(uuid())
  make      String
  model     String
  year      Int
  color     String
  price     Float
  userId    String
  user      User     @relation(fields: [userId], references: [id])
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
